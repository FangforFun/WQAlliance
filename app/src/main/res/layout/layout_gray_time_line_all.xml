<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:layout_marginTop="30dp"
    android:background="@color/W"
    tools:showIn="@layout/activity_check_process">

    <gkzxhn.wqalliance.mvp.widget.TimelineView
        android:id="@+id/timeline_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingBottom="74dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        app:marker="@drawable/marker_pass"
        app:marker_size="20dp"
        app:line_size="2dp"
        app:line="@color/BJ" />

    <include layout="@layout/layout_time_line_right_tv" />
</LinearLayout>