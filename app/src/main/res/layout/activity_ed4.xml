<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:orientation="vertical">

    <FrameLayout
        style="@style/fl_backgroud"/>

    <LinearLayout
        style="@style/ll_list">

        <RelativeLayout
            android:id="@+id/rl_ed4_1"
            style="@style/rl_item">

            <TextView
                style="@style/item_left_textview"
                android:layout_centerVertical="true"
                android:text="代理公司执照"/>

            <ImageView
                android:src="@drawable/add_pic_icon"
                android:layout_centerVertical="true"
                style="@style/item_right_arrow"/>

            <TextView
                style="@style/item_slip_line"/>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_ed4_2"
            style="@style/rl_item">

            <TextView
                style="@style/item_left_textview"
                android:layout_centerVertical="true"
                android:text="品牌保护合同"/>

            <ImageView
                android:src="@drawable/add_pic_icon"
                android:layout_centerVertical="true"
                style="@style/item_right_arrow"/>

            <TextView
                style="@style/item_slip_line"/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_ed4_3"
            style="@style/rl_item">

            <TextView
                style="@style/item_left_textview"
                android:layout_centerVertical="true"
                android:text="调查费发票"/>

            <ImageView
                android:src="@drawable/add_pic_icon"
                android:layout_centerVertical="true"
                style="@style/item_right_arrow"/>

            <TextView
                style="@style/item_slip_line"/>


        </RelativeLayout>

    </LinearLayout>
</FrameLayout>