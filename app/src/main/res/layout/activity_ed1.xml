<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:orientation="vertical">

    <FrameLayout
        style="@style/fl_bacgroud"/>

    <LinearLayout
        style="@style/ll_list">

        <RelativeLayout
            android:id="@+id/rl_ed1_1"
            style="@style/rl_item">

            <TextView
                style="@style/item_left_textview"
                android:layout_centerVertical="true"
                android:text="原告营业执照"/>

            <ImageView
                android:src="@drawable/add_pic_icon"
                android:layout_centerVertical="true"
                style="@style/item_right_arrow"/>

            <TextView
                style="@style/item_slip_line"/>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_ed1_2"
            style="@style/rl_item">

            <TextView
                style="@style/item_left_textview"
                android:layout_centerVertical="true"
                android:text="原告法定代表人证明"/>

            <ImageView
                android:src="@drawable/add_pic_icon"
                android:layout_centerVertical="true"
                style="@style/item_right_arrow"/>

            <TextView
                style="@style/item_slip_line"/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_ed1_3"
            style="@style/rl_item">

            <TextView
                style="@style/item_left_textview"
                android:layout_centerVertical="true"
                android:text="被告工商登记信息"/>

            <ImageView
                android:src="@drawable/add_pic_icon"
                android:layout_centerVertical="true"
                style="@style/item_right_arrow"/>

            <TextView
                style="@style/item_slip_line"/>


        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_ed1_4"
            style="@style/rl_item">

            <TextView
                style="@style/item_left_textview"
                android:layout_centerVertical="true"
                android:text="被告身份信息"/>

            <ImageView
                android:src="@drawable/add_pic_icon"
                android:layout_centerVertical="true"
                style="@style/item_right_arrow"/>

            <TextView
                style="@style/item_slip_line"/>

        </RelativeLayout>

    </LinearLayout>
</FrameLayout>